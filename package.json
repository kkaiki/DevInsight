{
  "name": "vscode-DevInsights",
  "displayName": "DevInsights",
  "description": "Metrics, insights, and time tracking automatically generated from your programming activity.",
  "version": "25.8.0",
  "publisher": "DevInsights",
  "author": {
    "name": "DevInsights"
  },
  "categories": [
    "Visualization",
    "Education"
  ],
  "keywords": [
    "analytics",
    "code",
    "code time",
    "codetime",
    "flow",
    "focus",
    "metrics",
    "productivity",
    "slack",
    "standup",
    "time",
    "timer",
    "time tracker",
    "time tracking",
    "todo",
    "tracker",
    "tracking",
    "worktime"
  ],
  "license": "BSD-3-Clause",
  "galleryBanner": {
    "color": "#AEC3D8",
    "theme": "light"
  },
  "extensionKind": [
    "workspace"
  ],
  "activationEvents": [
    "*"
  ],
  "engines": {
    "vscode": "^1.91.1",
    "sqlops": "*",
    "azdata": "*"
  },
  "main": "./dist/extension",
  "browser": "./dist/web/extension",
  "scripts": {
    "vscode:prepublish": "webpack --mode production",
    "compile": "webpack --mode none",
    "compile:dev": "tsc -p ./",
    "watch": "webpack --mode none --watch",
    "open-in-browser": "vscode-test-web --open-devtools --version sources --extensionDevelopmentPath=. .",
    "test": "npm run compile:dev && node ./node_modules/vscode/bin/test"
  },
  "contributes": {
    "commands": [
      {
        "command": "devinsights.discordid",
        "title": "DevInsights: Input Discord unique Id(数字のみの値です。開発者モードを有効にしてください)"
      }
    ],
    "configuration": {
      "title": "DevInsights",
      "properties": {
        "settings.discordId": {
          "type": "string",
          "description": "Your Discord ID",
          "scope": "application"
        }
      }
    }
  },
  "devDependencies": {
    "@types/adm-zip": "^0.4.34",
    "@types/azdata": "^1.46.6",
    "@types/mocha": "^10.0.7",
    "@types/node": "^17.0.45",
    "@types/request": "^2.48.12",
    "@types/vscode": "^1.91.1",
    "@types/webpack": "^5.28.5",
    "@types/which": "^2.0.2",
    "@vscode/test-web": "^0.0.22",
    "adm-zip": "0.5.9",
    "del-cli": "^4.0.1",
    "diff": "^5.2.0",
    "mkdirp": "^3.0.1",
    "mocha": "^10.7.3",
    "original-fs": "^1.2.0",
    "prettier": "2.6.0",
    "request": "^2.88.2",
    "ts-loader": "^9.5.1",
    "tslib": "^2.7.0",
    "typescript": "^4.9.5",
    "webpack": "^5.94.0",
    "webpack-cli": "^4.10.0",
    "which": "^2.0.2"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.645.0",
    "aws-sdk": "^2.1691.0",
    "dotenv": "^16.4.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/DevInsights.git"
  },
  "icon": "images/wakatime-logo.png",
  "bugs": {
    "url": "https://github.com/yourusername/DevInsights/issues"
  },
  "homepage": "https://github.com/yourusername/DevInsights#readme"
}
